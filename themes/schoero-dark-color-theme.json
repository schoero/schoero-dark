  "tokenColors": [
    {
      "name": "Dark gray - comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#636d83"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#a1a7b5"
      }
    },
    {
      "name": "Gray - Text color",
      "scope": [
        "source",                                                                // Everything
        "constant.other.color",
        "punctuation",                                                          // Curly braces and brackets and semicolon
        "keyword.operator",                                                     // Operators
        "meta.brace.square",                                                    // Square brackets
        "meta.brace.round",                                                     // Round brackets
        "variable.other.jsdoc",                                                 // JSDoc var
      ],
      "settings": {
        "foreground": "#b1b6c1"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Red",
      "scope": [
        "keyword",                                                              // Optional question mark, equals
        "meta.method.declaration",                                              // Method declaration
        "meta.object.member",                                                   // Object member
        "variable.other.object.property",                                       // Object property
        "variable.other.property",                                              // Class property
        "variable.language.super",                                              // Super
        "variable.language.this",                                               // this keyword
        "meta.tag",                                                             // HTML tags
        "meta.import",                                                          // Import
        "variable.other.readwrite.alias",                                       // Alias
        "punctuation.definition.template-expression",                           // Template expression: ${}
        "support.variable.property",                                            // Variable property: .lenght
        "support.type.property-name.json",                                      // JSON property name
        "support.type.property-name.json punctuation",                          // JSON property quotes
      ],
      "settings": {
        "foreground": "#E06C75"
      }
    },{
      "name": "Purple",
      "scope": [
        "storage.modifier",                                                     // extends, private public
        "storage.type.class",                                                   // Class keyword
        "storage.type.interface",                                               // Interface keyword
        "keyword.control",                                                      // export
        "meta.var.expr",                                                        // const, var, let
        "meta.method.declaration",                                              // Method declaration
        "storage.type.class.jsdoc",                                             // JSDoc params
        "keyword.operator.new",                                                 // new keyword
        "keyword.operator.expression.typeof",                                   // typeof keyword
        "keyword.operator.expression.of",                                       // of keyword
        "keyword.operator.expression.in",                                       // in keyword
        "keyword.operator.expression.instanceof",                               // instanceof keyword
        "keyword.operator.expression.delete",                                   // delete keyword
        "keyword.operator.expression.void",                                     // void keyword
      ],
      "settings": {
        "foreground": "#C678DD"
      }
    },{
      "name": "Blue",
      "scope": [
        "constant.language.boolean",                                            // true, false
        "meta.function-call",                                                   // function calls
        "entity.name.function",                                                 // function names
      ],
      "settings": {
        "foreground": "#61AFEF"
      }
    },{
      "name": "Turquoise",
      "scope": [
        "support.type.primitive",                                               // primitive type like string | number
        "meta.interface",                                                       // interfaces
        "support.class.builtin",                                                // built in classes
        "constant.language.json",                                               // JSON true false etc
      ],
      "settings": {
        "foreground": "#56B6C2"
      }
    },{
      "name": "Orange",
      "scope": [
        "variable.other.constant",                                              // constant variable name
        "constant.numeric",                                                     // numbers
        "entity.other.attribute-name",                                          // HTML attribute name
        "constant.numeric.decimal",                                             // Decimal numbers
        "constant.language.undefined",                                          // undefined keyword
      ],
      "settings": {
        "foreground": "#D19A66"
      }
    },{
      "name": "Yellow",
      "scope": [
        "meta.type.annotation",                                                 // Type annotation
        "entity.name.type"                                                      // Type name
      ],
      "settings": {
        "foreground": "#E5C07B"
      }
    },{
      "name": "Light green",
      "scope": [
        "string.quoted",                                                        // String literals
        "punctuation.definition.string",                                        // String quotes
        "entity.name.type.module",                                              // Imported module
        "meta.class",                                                           // class keyword
        "string.template",                                                      // Template strings
      ],
      "settings": {
        "foreground": "#98C379"
      }
    }
  ]
}
